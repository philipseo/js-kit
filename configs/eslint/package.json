{
  "name": "@philipseo/eslint-config",
  "version": "0.0.3",
  "license": "MIT",
  "author": "Philip Seo <hbnb7894@gmail.com>",
  "homepage": "https://github.com/philipseo/js-kit",
  "repository": {
    "type": "git",
    "url": "https://github.com/philipseo/js-kit",
    "directory": "configs/eslint-config"
  },
  "main": "./index.js",
  "files": [
    "src",
    "index.js"
  ],
  "exports": {
    ".": {
      "require": "./index.js"
    },
    "./base": {
      "require": "./src/base.js"
    },
    "./next": {
      "require": "./src/next.js"
    }
  },
  "scripts": {
    "eslint": "eslint .",
    "eslint:fix": "pnpm eslint --fix",
    "prettier": "prettier --ignore-path ../../.prettierignore --check .",
    "prettier:fix": "pnpm run prettier --write",
    "lint": "pnpm eslint && pnpm prettier",
    "lint:fix": "pnpm eslint:fix && pnpm prettier:fix",
    "release": "pnpm publish --no-git-checks --access public"
  },
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "6.4.0",
    "@typescript-eslint/parser": "6.4.0",
    "eslint": "8.47.0",
    "eslint-config-next": "^13.4.19",
    "eslint-config-prettier": "9.0.0"
  },
  "devDependencies": {
    "prettier": "3.0.2"
  }
}
