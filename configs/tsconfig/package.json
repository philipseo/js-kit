{
  "name": "@philipseo/tsconfig",
  "version": "0.0.2",
  "license": "MIT",
  "author": "Philip Seo <hbnb7894@gmail.com>",
  "homepage": "https://github.com/philipseo/js-kit",
  "repository": {
    "type": "git",
    "url": "https://github.com/philipseo/js-kit",
    "directory": "configs/tsconfig"
  },
  "main": "index.json",
  "files": [
    "./index.json",
    "./reactjs.json",
    "./nextjs.json"
  ],
  "engins": {
    "node": "18.x",
    "pnpm": "8.x"
  },
  "scripts": {
    "eslint": "eslint . --ignore-path ../../.eslintignore",
    "eslint:fix": "pnpm eslint --fix --ignore-path ../../.eslintignore",
    "prettier": "prettier --ignore-path ../../.prettierignore --check .",
    "prettier:fix": "pnpm run prettier --write --ignore-path ../../.prettierignore",
    "lint": "pnpm eslint && pnpm prettier",
    "lint:fix": "pnpm eslint:fix && pnpm prettier:fix",
    "release": "pnpm publish --no-git-checks --access public"
  },
  "devDependencies": {
    "@philipseo/eslint-config-jskit": "*",
    "@philipseo/prettier-config": "*"
  },
  "peerDependencies": {
    "typescript": "5.3.2"
  }
}
